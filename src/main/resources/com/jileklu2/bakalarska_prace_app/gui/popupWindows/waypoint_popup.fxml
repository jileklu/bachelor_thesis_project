<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="100.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jileklu2.bakalarska_prace_app.gui.routeHandling.WaypointWindowController">
   <children>
       <GridPane fx:id="gridPane" alignment="TOP_CENTER" hgap="10.0" layoutX="30.0" layoutY="22.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" vgap="5.0">
           <columnConstraints>
               <ColumnConstraints hgrow="SOMETIMES" maxWidth="150.0" minWidth="150.0" prefWidth="150.0" />
               <ColumnConstraints hgrow="SOMETIMES" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" />
               <ColumnConstraints hgrow="SOMETIMES" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" />
           </columnConstraints>
           <rowConstraints>
               <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
               <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
           </rowConstraints>
           <children>
               <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Coordinates" />
               <TextField fx:id="latTextField" onKeyTyped="#latTextFieldKeyTypedAction" prefHeight="25.0" prefWidth="100.0" promptText="Lat" GridPane.columnIndex="1" />
               <TextField fx:id="lngTextField" onKeyTyped="#lngTextFieldKeyTypedAction" prefHeight="25.0" prefWidth="100.0" promptText="Lng" GridPane.columnIndex="2" />
           </children>
           <padding>
               <Insets left="10.0" right="10.0" />
           </padding>
           <opaqueInsets>
               <Insets />
           </opaqueInsets>
       </GridPane>
       <Button fx:id="okButton" layoutX="150.0" layoutY="60.0" mnemonicParsing="false" onAction="#okButtonAction" text="OK" />
       <Button fx:id="cancelButton" layoutX="270.0" layoutY="60.0" mnemonicParsing="false" onAction="#cancelButtonAction" text="Cancel" />
   </children>
</AnchorPane>
