<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.web.*?>
<?import org.kordamp.ikonli.javafx.*?>

<BorderPane prefHeight="720.0" prefWidth="1280.0" style="-fx-background-color: #0f1114;" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jileklu2.bakalarska_prace_app.gui.MainViewController">
   <center>
      <WebView fx:id="webView" prefHeight="670.0" style="-fx-background-color: #0f1114;" BorderPane.alignment="CENTER" />
   </center>
   <top>
      <ButtonBar prefHeight="50.0" prefWidth="1280.0" style="-fx-background-color: #0f1114;" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets right="1000.0" />
         </BorderPane.margin>
      </ButtonBar>
   </top>
   <right>
      <ListView fx:id="listView" prefHeight="670.0" prefWidth="260.0" style="-fx-background-color: #0f1114;" stylesheets="@style_main_list_view.css" BorderPane.alignment="CENTER" />
   </right>
   <left>
      <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
             <Button fx:id="addWaypointButton" layoutY="58.0" mnemonicParsing="false" onAction="#addWaypointButtonAction" prefHeight="25.0" prefWidth="200.0" style="-fx-background-color: #0f1114;" text="Add Waypoint" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
               <graphic>
                  <FontIcon iconColor="WHITE" iconSize="20" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-icon-code: fa-map-marker;" wrappingWidth="24.428571701049805" />
               </graphic></Button>
             <Button fx:id="routeButton" layoutY="14.0" mnemonicParsing="false" onAction="#routeButtonAction" prefHeight="25.0" prefWidth="200.0" style="-fx-background-color: #0f1114;" text="Make route" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
               <graphic>
                  <FontIcon iconColor="WHITE" iconSize="20" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-icon-code: fa-car;" wrappingWidth="56.7142858505249" />
               </graphic></Button>
             <Button fx:id="dateButton" layoutY="102.0" mnemonicParsing="false" onAction="#dateButtonAction" prefHeight="25.0" prefWidth="200.0" style="-fx-background-color: #0f1114;" text="Set interval" textAlignment="CENTER" textFill="WHITE">
                 <font>
                     <Font name="System Bold" size="20.0" />
                 </font>
               <graphic>
                     <FontIcon iconColor="WHITE" iconSize="20" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-icon-code: fa-calendar;" wrappingWidth="50.7142858505249" />
               </graphic></Button>
             <Button fx:id="importButton" layoutY="568.0" mnemonicParsing="false" onAction="#importButtonAction" prefHeight="25.0" prefWidth="200.0" style="-fx-background-color: #0f1114;" text="Import route" textAlignment="CENTER" textFill="WHITE">
                 <font>
                     <Font name="System Bold" size="20.0" />
                 </font>
                 <graphic>
                     <FontIcon iconColor="WHITE" iconSize="20" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-icon-code: fa-file-o;" wrappingWidth="45.7142858505249" />
                 </graphic></Button>
             <Button fx:id="exportButton" layoutY="612.0" mnemonicParsing="false" onAction="#exportButtonAction" prefHeight="25.0" prefWidth="200.0" style="-fx-background-color: #0f1114;" text="Export route" textAlignment="CENTER" textFill="WHITE">
                 <font>
                     <Font name="System Bold" size="20.0" />
                 </font>
                 <graphic>
                     <FontIcon iconColor="WHITE" iconSize="20" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-icon-code: fa-file-code-o;" wrappingWidth="45.7142858505249" />
                 </graphic></Button>
         </children></Pane>
   </left>
