<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jileklu2.bakalarska_prace_app.ui.RoutePopUpController">
   <children>
      <BorderPane prefHeight="300.0" prefWidth="400.0">
         <top>
            <Text fx:id="mainText" strokeType="OUTSIDE" strokeWidth="0.0" text="Route parameters" BorderPane.alignment="CENTER">
               <font>
                  <Font name="MS Outlook" size="20.0" />
               </font></Text>
         </top>
         <bottom>
            <VBox prefHeight="71.0" prefWidth="400.0" BorderPane.alignment="CENTER">
               <children>
                  <HBox alignment="TOP_CENTER" prefHeight="54.0" prefWidth="400.0">
                     <children>
                        <Button fx:id="waypointBtn" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#waypointBtnAction" prefHeight="25.0" prefWidth="120.0" text="Add Waypoint" textAlignment="CENTER" />
                     </children>
                  </HBox>
                  <HBox alignment="TOP_CENTER" prefHeight="50.0" prefWidth="400.0" spacing="20.0">
                     <children>
                        <Button fx:id="okBtn" mnemonicParsing="false" onAction="#okBtnAction" prefHeight="25.0" prefWidth="80.0" text="OK" />
                        <Button fx:id="cancelBtn" mnemonicParsing="false" onAction="#cancelBtnAction" prefHeight="25.0" prefWidth="80.0" text="Cancel" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </bottom>
         <center>
            <ScrollPane hbarPolicy="NEVER" prefHeight="200.0" prefWidth="200.0" styleClass="grid" stylesheets="@style_scroll_pane.css" BorderPane.alignment="CENTER">
               <content>
                  <GridPane fx:id="gridPane" alignment="TOP_CENTER" hgap="10.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" vgap="5.0">
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" maxWidth="150.0" minWidth="150.0" prefWidth="150.0" />
                      <ColumnConstraints hgrow="SOMETIMES" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" />
                      <ColumnConstraints hgrow="SOMETIMES" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Origin Coordinates" />
                        <TextField fx:id="orgLatTextField" onKeyTyped="#originLatKeyTextFieldTypedAction" prefHeight="25.0" prefWidth="100.0" promptText="Lat" GridPane.columnIndex="1" />
                        <TextField fx:id="orgLngTextField" onKeyTyped="#originLngTextFieldKeyTypedAction" prefHeight="25.0" prefWidth="100.0" promptText="Lng" GridPane.columnIndex="2" />
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Destination Coordinates" GridPane.rowIndex="1" />
                        <TextField fx:id="destLatTextField" onKeyTyped="#destinationLatKeyTextFieldTypedAction" prefHeight="25.0" prefWidth="100.0" promptText="Lat" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <TextField fx:id="destLngTextField" onKeyTyped="#destinationLngKeyTextFieldTypedAction" prefHeight="25.0" prefWidth="100.0" promptText="Lng" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                     </children>
                     <padding>
                        <Insets left="10.0" right="10.0" />
                     </padding>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                  </GridPane>
               </content>
            </ScrollPane>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
