<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.web.*?>

<BorderPane prefHeight="720.0" prefWidth="1280.0" style="-fx-background-color: #0f1114;" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jileklu2.bakalarska_prace_app.ui.MainViewController">
   <center>
      <WebView fx:id="webView" prefHeight="393.0" prefWidth="594.0" style="-fx-background-color: #0f1114;" BorderPane.alignment="CENTER" />
   </center>
   <top>
      <ButtonBar prefHeight="50.0" prefWidth="1280.0" style="-fx-background-color: #0f1114;" BorderPane.alignment="CENTER">
        <buttons>
          <Button fx:id="routeButton" mnemonicParsing="false" onAction="#routeButtonAction" prefHeight="25.0" prefWidth="130.0" text="Make new route" />
            <Button mnemonicParsing="false" text="AddWaypoint" />
        </buttons>
         <BorderPane.margin>
            <Insets right="1000.0" />
         </BorderPane.margin>
      </ButtonBar>
   </top>
   <right>
      <ListView fx:id="listView" prefHeight="670.0" prefWidth="270.0" style="-fx-background-color: #0f1114;" stylesheets="@style_main_list_view.css" BorderPane.alignment="CENTER" />
   </right>
</BorderPane>
